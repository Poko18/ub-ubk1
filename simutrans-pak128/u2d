string=$(urllist https://sourceforge.net/projects/simutrans/files/pak128/ | grep 'pak128 for ' | sort -V | tail -n 1 | sed s/,.*//g | sed 's/.*for *//g')
head1=$(echo $string | sed s/.*\(//g)
rev=$(echo $string | sed 's/ .*//g')
revv=$(echo $string | sed s/" "*\(.*//g | sed 's/[A-Z]* *//g')
echo "${head1}.${rev}~$revv"
