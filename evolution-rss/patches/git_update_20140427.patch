+
+static void
+mail_part_rss_dispose (GObject *object)
+{
+        /*EMailPartAudio *part = E_MAIL_PART_AUDIO (object);
+
+        if (part->bus_id > 0) {
+                g_source_remove (part->bus_id);
+                part->bus_id = 0;
+        }
+
+        if (part->playbin != NULL) {
+                gst_element_set_state (part->playbin, GST_STATE_NULL);
